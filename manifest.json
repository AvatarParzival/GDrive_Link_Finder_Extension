{
  "manifest_version": 3,
  "name": "Google Drive Link Finder",
  "version": "1.2",
  "description": "Finds and tracks Google Drive links with history",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png"
    }
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "icons": {
    "16": "icon16.png"
  },
  "web_accessible_resources": [{
    "resources": ["history.html"],
    "matches": ["<all_urls>"]
  }]
}
